        <activity
            android:name=".UI.BuzzerActivity"
            android:label="@string/title_activity_buzzer" >
        </activity>
        <activity
            android:name=".UI.BuzzerStat2pActivity"
            android:label="@string/title_activity_buzzer_stat2p" >
